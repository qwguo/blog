<div class="article-main">
  <a class="iconfont icon-home" href="<%- url_for('/') %>"></a>
  <span class="article-doc-switch" id="docSwitch">
    <% for(let i = 0; i < 3; i++){%>
    <i></i>
    <% }%>
  </span>
  <%- partial('_partial/post/toc', {item: page}) %>
  <div class="article-tags">
    <div class="post-author">作者: <%- theme.author.name %></div>
    <%- partial('_partial/post/postProp', {item: page})%>
  </div>
  <div class="article-box">
    <%- page.content %>
  </div>
</div>



