.post-prop
    color darken(font-color, 60%)
    font-size 0
    margin 0
    padding 0
    list-style none
    li
      font-size 14px
      display inline-block
      margin 0 0 0 20px
      &:first-child
        margin 0
      a
        color inherit
    i
      color darken(font-color, 30%)
      margin-right 5px
